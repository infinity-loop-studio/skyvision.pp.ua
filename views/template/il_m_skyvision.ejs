<!DOCTYPE html>
<html>
<head>
    <title>Title</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/styles/custom.css">
    <link rel="stylesheet" href="/styles/uikit.css">
</head>
<body>

<nav class="uk-navbar-container uk-navbar" uk-navbar="" style="border-bottom: 1px solid #d09581;" uk-sticky>
    <div class="uk-navbar-left">

        <a class="uk-navbar-item uk-logo" href="/ru" style="padding: 5px;background: #875949;"><img style="height: 60px" src="/images/logo.png"></a>

        <ul class="uk-navbar-nav">
            <li>
                <a href="#" style="padding: 5px 10px;">
                    <span style="font-size: 16px;font-weight: bold;color: brown;" class="uk-text-uppercase">(066)445-21-18</span>
                </a>
            </li>
        </ul>

    </div>
    <div class="uk-navbar-right" style="background: #f1bf06;">
        <a
            style="
            color: brown;
            border-right: 1px solid #d09581;
            padding: 0 26px;
            border-left: 1px solid #d09581;"
            uk-icon="cog"
            class="uk-navbar-toggle uk-icon"
            href="#mobile-sidebar-menu"
            uk-toggle>
        </a>
        <a
            style="
            color: brown;
            padding: 0 26px;
            border-right: 1px solid #d09581;"
            class="uk-navbar-toggle uk-icon uk-navbar-toggle-icon"
            uk-navbar-toggle-icon
            href="#mobile-sidebar-menu"
            uk-toggle>
        </a>
    </div>
</nav>
<% if (positions.article) { %>
    <div uk-grid class="uk-background-default uk-margin-remove uk-padding">
        <article class="uk-width-expand@l uk-first-column uk-padding-remove-left uk-article">
            <%- positions.article %>
        </article>
        <% if (positions.sidebarright) { %>
            <div class="uk-width-1-3@l uk-padding-remove">
                <%- positions.sidebarright %>
            </div>
        <% } %>
    </div>
<% } %>
<% if (positions.section1){for (var i = 0; i < positions.section1.length; i++ ){ %>
    <%- positions.section1[i] %>
<% }} %>
<div id="mobile-sidebar-menu" uk-offcanvas="mode: reveal; overlay: true; flip: true">
    <div class="uk-offcanvas-bar">
        <button class="uk-offcanvas-close" type="button" uk-close></button>
        <%- positions.sidebarright %>
    </div>
</div>
<script src="/js/uikit.js"></script>
<script src="/js/uikit-icons.js"></script>
<script src="/js/fontawesome/all.min.js"></script>
</body>
</html>
